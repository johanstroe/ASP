@model IEnumerable<Member>

@if (!string.IsNullOrWhiteSpace(Context.Request.Query["query"]))
{
    <p>Sökresultat för: <strong>@Context.Request.Query["query"]</strong></p>
}

@{
    ViewData["Title"] = "Team Members";
}

<section id="team-members">
    <header>
        <h2>Team Members</h2>
        <button type="button" data-modal="true" data-target="#addMemberModal" class="btn btn-blue btn-add">Add Member</button>
    </header>
    <div class="surface">
        
        <partial name="Partials/Sections/_MemberList" model="Model" />
    </div>
</section>

